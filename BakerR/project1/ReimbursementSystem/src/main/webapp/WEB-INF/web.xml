<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>reimbursement</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Handles Logins</description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.revature.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
    <url-pattern></url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Logout functionality</description>
    <display-name>LogoutServlet</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.revature.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Account Registration</description>
    <display-name>RegistrationServlet</display-name>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>com.revature.servlet.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>User homepage</description>
    <display-name>UserHomeServlet</display-name>
    <servlet-name>UserHomeServlet</servlet-name>
    <servlet-class>com.revature.servlet.UserHomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserHomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>View accounts</description>
    <display-name>AccountListServlet</display-name>
    <servlet-name>AccountListServlet</servlet-name>
    <servlet-class>com.revature.servlet.AccountListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AccountListServlet</servlet-name>
    <url-pattern>/view-accts</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Handles account info updates</description>
    <display-name>UpdateAccountServlet</display-name>
    <servlet-name>UpdateAccountServlet</servlet-name>
    <servlet-class>com.revature.servlet.UpdateAccountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateAccountServlet</servlet-name>
    <url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Handles new reimbursement requests</description>
    <display-name>ReimbursementRequestServlet</display-name>
    <servlet-name>ReimbursementRequestServlet</servlet-name>
    <servlet-class>com.revature.servlet.ReimbursementRequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReimbursementRequestServlet</servlet-name>
    <url-pattern>/request</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ReimbursementListServlet</display-name>
    <servlet-name>ReimbursementListServlet</servlet-name>
    <servlet-class>com.revature.servlet.ReimbursementListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReimbursementListServlet</servlet-name>
    <url-pattern>/view-reimbursement</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UpdateReimbursementServlet</display-name>
    <servlet-name>UpdateReimbursementServlet</servlet-name>
    <servlet-class>com.revature.servlet.UpdateReimbursementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateReimbursementServlet</servlet-name>
    <url-pattern>/update-request</url-pattern>
  </servlet-mapping>
</web-app>