select * from ers_reimbursement_status;
select * from ers_reimbursement_type;
select * from ers_user_roles;
select * from ers_users;
select r_id, r_amount, r_description, (r_receipt is not null) as has_receipt,
       length(r_receipt) as receipt_size, r_receipt_name, r_submitted, u_id_author,
       r_resolved, u_id_resolver, rt_type, rt_status
    from ers_reimbursements;


