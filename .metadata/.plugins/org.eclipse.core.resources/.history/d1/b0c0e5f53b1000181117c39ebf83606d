package com.revature;

public class Basics {
	public static void main(String[] args) {
		//Naming convention
		//Classes and Projects - PascalCase, capitalize the first letter of every
		//		word.	EX. FirstSecondThird
		//Methods and Variables: camelCase		EX. firstSecondThird
		//Packages - all lower case separated by periods
		//Constants - ALL CAPS
		//
		
	}
}
