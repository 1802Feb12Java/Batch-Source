package com.revature.classtypes;

import java.util.ArrayList;

public class Driver {

	public static void main(String[] args) {
		String[] fishTypes = {"One","two","red","blue"};
		Guppie bob = new Guppie("Bob","Clown");
		
		ArrayList<Guppie> hotdog = new ArrayList<Guppie>();
		
		bob.eat();
		bob.swim(9001);
		bob.typeOfFFFFFFFFFFFFFIIIIIIIIIIIIIIIIIISSSSSSSSSSSSSSSSSSSHHHHHHHHHHHHHHHHHHH(fishTypes);
		System.out.println(bob.getName());
		
		fishToHotdog(bob, hotdog);
	}

	public static void fishToHotdog(Guppie g, ArrayList<Guppie> a) {
		a.add(g);
		System.out.println("Added guppie.");
	}
}
