package com.revature;

public class Q10 {
	public void run() {
		double[] numArr = {0.457, 5.78345, 10.4516, 1.645132, 4.651, 9.1237, 3.119, 7.546, 8.789};
		
		int result;
		boolean boo = (numArr[0] < numArr[1]);
		for(int i=0; i<numArr.length-1; i++) {
			result = findMin(numArr[i], numArr[i+1]);
			if(result == -1) {
				System.out.println("min of " + numArr[i] + " vs " + numArr[i+1] + ": " + numArr[i]);
			}
			else if(result == 1) {
				System.out.println("min of " + numArr[i] + " vs " + numArr[i+1] + ": " + numArr[i+1]);
			}
			else {
				System.out.println("WHAT HAVE YOU DONE");
			}
		}
	}
	
	public int findMin(double a, double b) {
		return a<=b ? -1:1;
	}
}
