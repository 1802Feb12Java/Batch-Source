package com.revature;

import java.util.Scanner;

public class Q14 {
	public void run() {
		Scanner scan = new Scanner(System.in);
		System.out.print("Hi. Please input 1, 2, or 3: ");
		int choice = scan.nextInt();
		switch(choice) {
			case 1:
				System.out.println("Please input any number: ");
				double numToRoot = scan.nextDouble();	
				System.out.println(Math.pow(numToRoot, 0.5));	//raise it to the 1/2 power to take square root
				break;
			case 2:
				
				break;
			case 3:
				
				break;
		}
	}
}
