package com.revature;

public class Chainz {
	
	public static void main(String[] args) {
		Mouse mouse = new Mouse(6000, 10, 8);
		Mouse mighty = new Mouse(5, 564);
		Mouse mouse3 = new Mouse(5);
		
		System.out.println(mouse.toString());
		System.out.println(mighty.toString());
		System.out.println(mouse3.toString());
	}
}
