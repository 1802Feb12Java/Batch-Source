package com.revature;

public class Q2 {
	private int numFibs = 0; //keep track of how many times the fibonnacci sequence was done
	
	public void run() {
		fibonacci(0, 1, 25);
	}
	
	public void fibonacci(int start, int last, int length) {
		numFibs++;
		System.out.print(start + ", ");
		start += last;
		if(numFibs < length) {
			fibonacci(start, last, length);
		}
	}
}
