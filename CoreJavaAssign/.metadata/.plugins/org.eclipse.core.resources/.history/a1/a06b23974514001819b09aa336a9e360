package com.revature.core.java.q7;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		Employees michael = new Employees("Michael", "Regional Manager", 45);
		Employees dwight = new Employees("Dwight", "Assistant to the Regional Manager", 43);
		
		ArrayList<Employees> employeesList = new ArrayList<Employees>();
		employeesList.add(michael);
		employeesList.add(dwight);
		
		/*
		 * loop through each element of employeesList, and prompt user for input
		 */
		Collections.sort(employeesList, new EmployeesSortName());

	}	//end of main
}
