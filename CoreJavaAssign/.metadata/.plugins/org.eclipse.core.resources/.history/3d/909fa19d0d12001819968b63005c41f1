package com.revature;

/*
 * Johne Vang
 * Q4 N Factorial
 * Write a program to compute N factorial.
 */
public class Q4_NFactorial {
	public static void main(String[] args) {
		factorial(4);
	}
	
	/**
	 * 
	 * @param num
	 * @return
	 */
	public static void factorial(int num) {
		
		if(num == 1) {
			1;
		} else {
			factorial(num * (num - 1));
		}
		return result;
	}
}
